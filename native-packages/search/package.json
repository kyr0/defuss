{
  "name": "defuss-search",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "bun@1.3.9",
  "description": "Hybrid Text & Vector Search",
  "repository": "https://github.com/kyr0/defuss",
  "scripts": {
    "prebuild": "rm -rf ./pkg",
    "build": "wasm-pack build --target web --out-dir pkg",
    "postbuild": "pkgroll",
    "clean": "rm -rf pkg",
    "decompile": "wasm2wat --generate-names --verbose --enable-threads pkg/defuss_search_bg.wasm -o pkg/defuss_search_bg.wast > pkg/defuss_search_bg.log 2>&1",
    "pretest": "bun build",
    "test": "cargo test",
    "prebench": "bun build && mkdir -p public && cp -R tools/WebFAQ/dataset/*_flat.json public/",
    "bench": "vitest run --config vitest.bench.config.ts",
    "release": "sh scripts/release.sh",
    "embedding-server": "ollama serve"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./wasm": {
      "import": {
        "types": "./pkg/defuss_search.d.ts",
        "default": "./pkg/defuss_search.js"
      }
    },
    "./js": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "pkg/*",
    "dist/*"
  ],
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@types/node": "^25.0.9",
    "@types/web": "^0.0.319",
    "@vitest/browser": "^4.0.17",
    "@vitest/browser-playwright": "^4.0.17",
    "pkgroll": "^2.21.5",
    "playwright": "^1.57.0",
    "tinybench": "^6.0.0",
    "vitest": "^4.0.17",
    "wasm-pack": "^0.14.0",
    "wasm2wat": "^1.1.1"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "search",
    "wasm",
    "multicore",
    "rust",
    "javascript",
    "typescript",
    "webassembly",
    "browser",
    "web-workers"
  ],
  "author": "Aron Homberg <info@aron-homberg.de>",
  "license": "MIT"
}