{
  "name": "defuss-fastmath",
  "version": "0.0.1",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "packageManager": "pnpm@9.14.2",
  "description": "Ludicrously fast multicore math for web browsers using WebAssembly, SIMD-128 and Web Workers",
  "repository": "https://github.com/kyr0/defuss",
  "files": ["index.js", "index.d.ts", "pkg/"],
  "scripts": {
    "prebuild": "rm -rf ./pkg",
    "build": "wasm-pack build --target web --out-dir pkg",
    "postbuild": "pnpm decompile",
    "clean": "rm -rf pkg",
    "decompile": "wasm2wat --enable-threads pkg/defuss_fastmath_bg.wasm -o pkg/defuss_fastmath_bg.wast",
    "pretest": "pnpm build",
    "test": "vitest run",
    "prebench": "pnpm test",
    "bench": "vitest run --config vitest.bench.config.ts"
  },
  "devDependencies": {
    "@vitest/browser": "^3.2.3",
    "@testing-library/dom": "^10.4.0",
    "playwright": "^1.53.0",
    "vitest": "^3.2.3",
    "wasm-pack": "^0.12.1",
    "tinybench": "^4.0.1"
  },
  "engines": {
    "node": ">= 10"
  },
  "keywords": [
    "fastmath",
    "math",
    "wasm",
    "multicore",
    "rust",
    "javascript",
    "typescript",
    "webassembly",
    "browser",
    "web-workers",
    "audio",
    "dsp",
    "ml",
    "matmul",
    "oscillators"
  ],
  "author": "Aron Homberg <info@aron-homberg.de>",
  "license": "MIT"
}
