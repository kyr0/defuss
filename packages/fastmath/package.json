{
  "name": "defuss-fastmath",
  "version": "0.0.1",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "packageManager": "pnpm@9.14.2",
  "description": "Ludicrously fast multicore math for Rust, JavaScript, TypeScript, browsers, and Node.js",
  "repository": "https://github.com/kyr0/defuss",
  "files": ["index.js", "index.d.ts", "pkg/"],
  "scripts": {
    "prebuild": "rm -rf ./pkg && rm -rf ./pkg-bundler && rm -rf ./pkg-node",
    "build": "wasm-pack build --target web --out-dir pkg",
    "build:bundler": "wasm-pack build --target bundler --out-dir pkg-bundler",
    "build:nodejs": "wasm-pack build --target nodejs --out-dir pkg-node",
    "build:all": "npm run build && npm run build:bundler && npm run build:nodejs",
    "clean": "rm -rf pkg pkg-bundler pkg-node",
    "pretest": "npm run prebuild",
    "test": "wasm-pack test --node",
    "dev": "npm run build && node test.js"
  },
  "devDependencies": {
    "wasm-pack": "^0.12.1",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "keywords": [
    "fastmath",
    "math",
    "wasm",
    "multicore",
    "rust",
    "javascript",
    "typescript",
    "nodejs",
    "browser",
    "audio",
    "dsp",
    "ml",
    "matmul",
    "oscillators"
  ],
  "author": "Aron Homberg <info@aron-homberg.de>",
  "license": "MIT"
}
