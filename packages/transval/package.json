{
  "name": "defuss-transval",
  "version": "1.0.5",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "description": "A fast, functional, reliable and small state (e.g. form) validation library that aims for correctness and simplicity.",
  "keywords": [
    "validation",
    "form",
    "async",
    "extensibility",
    "tiny",
    "correctness",
    "functional",
    "flexibility"
  ],
  "repository": {
    "url": "git+https://github.com/kyr0/defuss.git",
    "type": "git"
  },
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./node_modules/.pnpm",
    "pretest": "pnpm run build",
    "prebuild": "pnpm run clean",
    "build": "pkgroll",
    "test": "vitest --run  --coverage"
  },
  "author": "Aron Homberg <info@aron-homberg.de>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": ["dist"],
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >=21.0.0"
  },
  "dependencies": {
    "defuss-runtime": "^1.0.0"
  },
  "devDependencies": {
    "defuss": "^2.0.3",
    "pkgroll": "^2.5.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^3.1.3",
    "happy-dom": "^15.11.7",
    "@vitest/coverage-v8": "^3.1.3"
  }
}
